<launch>

<node pkg="um6" type="um6_driver" name="um6_driver" output="screen" respawn="true">
	<param name="port" type="string" value="/dev/ttyUSB0"/>
	<param name="baud" type="int" value="115200"/>
	<param name="mag_updates" type="boolean" value="true"/>
	<param name="accel_updates" type="boolean" value="true"/>
</node>

<node pkg="tf" type="static_transform_publisher" name="imu_link_publisher" args="0 0 0 0 0 0 base_link imu_link 10"/>


</launch>
